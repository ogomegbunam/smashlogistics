<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.smashlogistics.app">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name="com.smashlogistics.app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.packaged.NoActionBar"
        android:usesCleartextTraffic="true"
      >
        <activity
            android:name="com.smashlogistics.app.activity.DriverSearchActivity"
            android:exported="false" />
        <activity
            android:name="com.smashlogistics.app.activity.BookWheelerActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.CoupunActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.ReviewMapActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.DropMapActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.TripDetailsActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.SendPackageActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.MapsActivityCurrentPlace"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.HomeActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.LoginActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.IntroActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.SendOTPActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar" />
        <activity
            android:name="com.smashlogistics.app.activity.FirstActivity"
            android:exported="true"
            android:theme="@style/Theme.packaged.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.smashlogistics.app.activity.PaypalActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.PaystackActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.RazerpayActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.StripPaymentActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.FlutterwaveActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.SenangpayActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.PaytmActivity"
            android:exported="true" />
        <activity
            android:name="com.smashlogistics.app.activity.HelpDetailsActivity"
            android:exported="true" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/API_KEY" />

        <activity
            android:name="com.smashlogistics.app.MainActivity"
            android:exported="true" />
    </application>

</manifest>